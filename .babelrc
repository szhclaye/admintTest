{
    "presets": ["env", "react", "stage-0"],
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true
            }
        ],
        [
            "import",
            { "libraryName": "antd", "libraryDirectory": "es", "style": "true" }   
        ],
        "react-hot-loader/babel",
        "transform-object-rest-spread"
    ],
    "env": {
        "development": {
            "plugins": ["react-hot-loader/babel"]
        }
    }
}

